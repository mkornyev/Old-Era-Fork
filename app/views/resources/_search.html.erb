<%= simple_form_for(@resources) do |f| %>
  <%= f.error_notification %>
  <div class="row">
    <div class="form-inputs">
      <%= f.input :search %>
    </div>
  </div>
  <div class="row">
    <div class="form-actions col s8">
      <%= f.button :submit, input_html: { formaction: '/resources' } %>
    </div>
  </div>
<% end %>



<p id="notice"><%= notice %></p>

<h1>Outreach Workers</h1>

<table>
  <thead>
    <tr>
      <th>Organization</th>
      <th>User</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @outreach_workers.each do |outreach_worker| %>
      <tr>
        <td><%= outreach_worker.organization %></td>
        <td><%= outreach_worker.user %></td>
        <td><%= link_to 'Show', outreach_worker %></td>
        <td><%= link_to 'Edit', edit_outreach_worker_path(outreach_worker) %></td>
        <td><%= link_to 'Destroy', outreach_worker, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Outreach Worker', new_outreach_worker_path %>
